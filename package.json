  "scripts": {
    "postinstall": "npm run prepare",
    "compile:browser": "webpack --config=./webpack.browser.config.js --progress --color",
    "compile:node": "webpack --config=./webpack.node.config.js --progress --color",
    "compile:worker-host": "webpack --config=./webpack.worker-host.config.js --progress --color",
    "compile:ext-host": "webpack --config=./webpack.ext-host.config.js --progress --color",
    "prepare": "npm run compile:worker-host && npm run compile:ext-host",
    "build": "npm run compile:browser && npm run compile:node && npm run compile:worker-host && npm run compile:ext-host",
    "start:client": "webpack-dev-server --config=./webpack.browser.config.js --progress --color --watch",
    "start:server": "cross-env NODE_ENV=development EXT_MODE=js KTLOG_SHOW_DEBUG=1 node --inspect=9991 -r ts-node/register ./src/node/index.ts",
    "start": "run-p start:client start:server"
  },
